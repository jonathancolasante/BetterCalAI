{
  "expo": {
    "name": "Personal Food Log",
    "slug": "personal-food-log",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android", "web"],
    "assetBundlePatterns": ["**/*"],
    "sdkVersion": "54.0.0",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "We need access to take a picture of your food."
        }
      ]
    ],
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "ios": {
      "bundleIdentifier": "com.example.personalfoodlog",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to take a picture of your food."
      }
    },
    "android": {
      "package": "com.example.personalfoodlog",
      "permissions": ["CAMERA"]
    }
  }
}
